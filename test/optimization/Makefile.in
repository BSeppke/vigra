curdir=$(vigra_testdir)/optimization

test: test.o
	$(CXX) -o test test.o
	./test || { rm ./test$(EXEEXT) ; exit 1; }

math.exe: test.d
	devenv "..\test.sln" /build Release /project optimization
	./optimization.exe || { rm ./optimization.exe ; exit 1; }

ifneq "$(MAKECMDGOALS)" "clean"
include test.d
endif

include ../Makefile.include
