include ../config/Makefile.include

all:: impex

install::
	@cd impex ; $(MAKE) install

impex::
	@cd impex ; $(MAKE) all

examples::
	@cd examples ; $(MAKE) CXX=$(CXX) all

clean::
	@cd impex ; $(MAKE) clean
	@cd examples ; $(MAKE) clean

distclean::
	@cd impex ; $(MAKE) distclean
	@cd examples ; $(MAKE) distclean
